    "claude-code",
    "memory",
    "vector-search",
    "pgvector",
    "postgresql",
    "embeddings"
  ],
  "author": "Matt Dionis <mattdionis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Matt-Dionis/claude-code-configs.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "claude-config": {
    "version": "1.0.0",
